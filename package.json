{
  "name": "vite-plugin-excel-to-i18n",
  "version": "0.1.3",
  "description": "Vite plugin to convert Excel files to i18n JSON files",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "wasm/pkg",
    "scripts"
  ],
  "scripts": {
    "build": "npm run build:wasm && tsup src/index.ts --format cjs,esm --dts && node scripts/copy-wasm.js",
    "build:wasm": "cd wasm && wasm-pack build --target bundler --out-dir pkg",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "postinstall": "node scripts/check-wasm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gyuha/vite-plugin-excel-to-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/gyuha/vite-plugin-excel-to-i18n/issues"
  },
  "homepage": "https://github.com/gyuha/vite-plugin-excel-to-i18n#readme",
  "keywords": [
    "vite-plugin",
    "i18n",
    "excel",
    "internationalization",
    "localization",
    "webassembly",
    "wasm"
  ],
  "author": "Gyuha <nicegyuha@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "wasm-pack": "^0.12.1"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  }
}
