{
  "name": "vite-plugin-excel-to-i18n",
  "version": "0.1.0",
  "description": "Excel to i18n converter plugin for Vite",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src/wasm/pkg"
  ],
  "scripts": {
    "build:wasm": "wasm-pack build src/wasm --target nodejs --out-dir pkg",
    "build:ts": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "clean:wasm": "node -e \"const fs=require('fs');const path=require('path');const dir=path.resolve('dist/wasm');if(fs.existsSync(dir)){fs.rmSync(dir,{recursive:true,force:true});}fs.mkdirSync(path.resolve('dist/wasm/pkg'),{recursive:true});\"",
    "build": "npm run clean:wasm && npm run build:wasm && npm run build:ts && npm run copy:wasm",
    "copy:wasm": "copyfiles -f \"src/wasm/pkg/*\" dist/wasm/pkg/",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "pack:manual": "node scripts/pack.js",
    "test:wasm": "node test/node-debug.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gyuha/vite-plugin-excel-to-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/gyuha/vite-plugin-excel-to-i18n/issues"
  },
  "homepage": "https://github.com/gyuha/vite-plugin-excel-to-i18n#readme",
  "keywords": [
    "vite-plugin",
    "i18n",
    "excel",
    "wasm",
    "webassembly",
    "rust"
  ],
  "author": "Gyuha <nicegyuha@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^14.18.0",
    "copyfiles": "^2.4.1",
    "lodash-es": "^4.17.21",
    "read-excel-file": "^5.7.0",
    "tsup": "^8.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.0",
    "wasm-pack": "^0.11.0"
  },
  "peerDependencies": {
    "lodash-es": "^4.17.0",
    "read-excel-file": "^5.0.0",
    "vite": "^2.0.0 || ^3.0.0 || ^4.0.0"
  }
}
